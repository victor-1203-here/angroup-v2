{   
    "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 150,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "importOrder": ["^react", "^next", "^@?\\w", "^@/", "^[./]", "^../../", "^[../]"],
    "importOrderSortSpecifiers": true
}
